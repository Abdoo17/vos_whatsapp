# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
# Commented out route generated by play
# GET     /                           controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)


# Generated routes
POST  /signup                          com.vangav.vos_whatsapp.controllers.signup.ControllerSignup.postSignup()
GET   /get_user_info                     com.vangav.vos_whatsapp.controllers.get_user_info.ControllerGetUserInfo.getGetUserInfo()
POST  /send_message                     com.vangav.vos_whatsapp.controllers.send_message.ControllerSendMessage.postSendMessage()
GET   /get_messages                     com.vangav.vos_whatsapp.controllers.get_messages.ControllerGetMessages.getGetMessages()
